{
  "compilerOptions": {
    "lib": ["es2018"],
    "target": "es2018",
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": true,
    "module": "ES6",
    "moduleResolution": "node",
    "declaration": true,
    "outDir": "./dist",
    "declarationDir": "./dist",
    "declarationMap": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
